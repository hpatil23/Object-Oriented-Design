<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="400.0" prefWidth="328.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hotel_management.Manager.remove_res">
   <top>
      <HBox alignment="CENTER" prefHeight="45.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Remove Resource" />
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="45.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="remove" mnemonicParsing="false" onAction="#remove" text="Remove Resource" />
            <Button fx:id="close" mnemonicParsing="false" onAction="#close" text="Close" />
         </children>
      </HBox>
   </bottom>
   <center>
      <Pane prefHeight="310.0" prefWidth="483.0" BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="allResource" layoutX="64.0" layoutY="38.0" prefHeight="200.0" prefWidth="200.0" />
            <Label layoutX="49.0" layoutY="260.0" text="Ctrl+Left Mouse Click for Multiple Selection" />
         </children>
      </Pane>
   </center>
</BorderPane>
